<?xml version="1.0"?>

<!--
Launchifle launches the necessary nodes for setting a youbot
-->

<launch>

	<arg name="robot_model"   default="$(env MR_ROBOT_MODEL)"/>
	<arg name="robot_id" default="$(env MR_ROBOT_ID)"/>
    <arg name="robot_ns" default="$(arg robot_model)_$(arg robot_id)" />


    <remap from="/cmd_vel" to="$(arg robot_ns)/input/cmd_vel" />
    <remap from="/odom" to="$(arg robot_ns)/input/odom" />
    <include file="$(find youbot_driver_ros_interface)/launch/youbot_driver.launch"/>

</launch>
