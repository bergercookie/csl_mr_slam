<?xml version="1.0"?>

<!--
Launchifle launches the necessary nodes for setting a youbot in a MR-SLAM
framework

-->

<launch>

<arg name = "robot_ns" value = "$(env MR_ROBOT_MODEL)_$(env MR_ROBOT_ID)" />

<remap from="/cmd_vel" to="$(arg robot_ns)/input/cmd_vel" />
<remap from="/odom" to="$(arg robot_ns)/input/cmd_vel" />
<include file="$(find youbot_driver_ros_interface)/launch/youbot_driver.launch"/>

</launch>
